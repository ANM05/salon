<div>
<h1>List of stylists</h1>
    <div class="list-group">
        #foreach ($stylist in $stylists)
        <a href="/stylist/$stylist.getId()" class="list-group-item list-group-item-action list-group-item-light">$stylist.getName()</a>
        #end
    </div>
    <a href="/stylists/new"><button class="btn-lg">Add new Stylist</button></a>
</div>