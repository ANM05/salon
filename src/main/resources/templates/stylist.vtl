<div class="container">
<h1 style="color: white;"><strong>List of stylists</strong></h1>
    <div class="list-group">
        #foreach ($stylist in $stylists)
        <a href="/stylist/$stylist.getId()" class="list-group-item list-group-item-action list-group-item-light">$stylist.getName()</a>
        #end
    </div>
    <br/>
    <a href="/stylists/new"><button class="btn btn-secondary">Add new Stylist</button></a>
</div>